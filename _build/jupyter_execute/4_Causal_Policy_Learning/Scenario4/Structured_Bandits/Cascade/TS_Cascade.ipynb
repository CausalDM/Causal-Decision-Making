   "source": [
    "K = env.K\n",
    "L = env.L\n",
    "u_prior_alpha = np.ones(L)\n",
    "u_prior_beta = np.ones(L)\n",
    "seed = 0\n",
    "\n",
    "TS_agent = TS_Cascade.TS_Cascade(K = K, L = L, u_prior_alpha = u_prior_alpha, \n",
    "                                 u_prior_beta = u_prior_beta, seed = seed)"
   ]
